grafana-service-yaml(parent, vars) ::= <<
apiVersion: v1
kind: Service
metadata:
  name: grafana
  namespace: monitoring
  labels:
    kubernetes.io/cluster-service: "true"
    addonmanager.kubernetes.io/mode: Reconcile
    kubernetes.io/name: "prometheus-operator"
spec:
  ports:
  - name: http
    port: 3000
    targetPort: http
  selector:
    app: grafana

>>
